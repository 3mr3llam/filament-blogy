{
    "name": "filament/blogy",
    "description": "A Laravel blog package with SEO features and Filament integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Amr Allam",
            "email": "iacoder@live.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "filament/filament": "^3.0",
        "filament/forms": "^3.0",
        "filament/tables": "^3.0",
        "filament/notifications": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "FilamentBlogy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilamentBlogy\\FilamentBlogyServiceProvider"
            ]
        }
    }
}
